<%= content_for :title, 'Listing Contact Types' %>

<%= link_to 'New Contact type', new_admin_contact_type_path %>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @contact_types.each do |contact_type| %>
  <tr>
    <td><%= contact_type.name %></td>
    <td><%= link_to 'Edit', edit_admin_contact_type_path(contact_type) %></td>
    <td><%= link_to 'Delete', admin_contact_type_path(contact_type), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
